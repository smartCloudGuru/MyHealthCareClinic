        <script>
            $(document).ready(function() {
                CalculateSum();
             });
              
            function CalculateSum(){ 
                for(var k=0; k<23; k++){
                    var totalDays = 0; 
                    var tblRows = $('[id$="pbtId'+k+'"] table tr').length-2;
                    //console.log('------1661',tblRows);
                    var rowSum = 0;
                    for(var j = 0;j<tblRows ;j++){  
                        rowSum = 0;                
                        for(var i=1;i<8;i++){   
                                              
                            rowSum += parseFloat($('.pbtId'+k+'_detailDay'+i+'_'+j).text());                         
                        }
                        $('.pbtId'+k+'_detailTotal_'+j).text(rowSum);
                        
                    }
                    for(var i=1;i<8;i++){ 
                        var sumdetailDay = 0;                
                        for(var j = 0;j<tblRows ;j++){
                            var colVal = parseFloat($('.pbtId'+k+'_detailDay'+i+'_'+j).text());
                            sumdetailDay+=colVal;     
                        }
                        $('.pbtId'+k+'_totdetailDay'+i).text(sumdetailDay);                           
                    }  
                    var totalSum = 0;
                    $('[class*="pbtId'+k+'_detailTotal_"]').each(function(){
                        totalSum  += parseFloat($(this).text());
                    });
                    $('.pbtId'+k+'_totdetailTotal').text(totalSum);
                 }
                 //console.log('--------0',parseFloat($('.pbtId0_detailDay3_12').text())/parseFloat($('.pbtId1_detailDay3_12').text()));
                 //console.log('--------1',parseFloat($('.pbtId1_detailDay3_12').text()));
                 CalculateConversion();
            } 
             function CalculateConversion(){ 
                var cellVal = 0;
                var tblRows = $('[id$="pbtId0"] table tr').length-2; 
                for(var i=0; i<tblRows; i++){
                    for(var j=1; j<8; j++){   
                        if(parseFloat($('.pbtId0_detailDay'+j+'_'+i).text()) === 0){
                            $('.pbtId2_detailDay'+j+'_'+i).text('0%'); 

                        }else{  
                           if(parseFloat($('.pbtId1_detailDay'+j+'_'+i).text()) === 0){
                               $('.pbtId2_detailDay'+j+'_'+i).text('0%');
                           }else{
                               $('.pbtId2_detailDay'+j+'_'+i).text((((parseFloat($('.pbtId1_detailDay'+j+'_'+i).text())/parseFloat($('.pbtId0_detailDay'+j+'_'+i).text()))*100).toFixed(2))+'%');
                           } 
                       }
                    }
                    if($('.pbtId0_detailTotal_'+i).text() == undefined || $('.pbtId0_detailTotal_'+i).text() == null || parseFloat($('.pbtId0_detailTotal_'+i).text()) == 0 ){
                        $('.pbtId2_detailTotal_'+i).text('0.00%');
                    }else{
                        $('.pbtId2_detailTotal_'+i).text((((parseFloat($('.pbtId1_detailTotal_'+i).text())/parseFloat($('.pbtId0_detailTotal_'+i).text()))*100).toFixed(2))+'%');
                    } 
                }  
                for(var i=1; i<8; i++){
                    if(parseFloat($('.pbtId1_totdetailDay'+i).text()) === 0  ){
                        $('.pbtId2_totdetailDay'+i).text('0%'); 
                    }else{
                        if(parseFloat($('.pbtId1_totdetailDay'+i).text()) === 0 && parseFloat($('.pbtId0_totdetailDay'+i).text()) === 0){
                            $('.pbtId2_totdetailDay'+i).text('0%');
                        }else{
                            $('.pbtId2_totdetailDay'+i).text((((parseFloat($('.pbtId1_totdetailDay'+i).text())/parseFloat($('.pbtId0_totdetailDay'+i).text()))*100).toFixed(2))+'%');
                        }
                    }
                    if(parseFloat($('.pbtId0_totdetailTotal').text()) === 0){
                        $('.pbtId2_totdetailTotal').text('0.00%');
                    }else{
                        $('.pbtId2_totdetailTotal').text((((parseFloat($('.pbtId1_totdetailTotal').text()))/(parseFloat($('.pbtId0_totdetailTotal').text()))*100).toFixed(2))+'%'); 
                    }
                    var totdetailDay = $('.pbtId0_totdetailDay'+i).text(); 
                    $('.pbtId01_totdetailDay'+i).text($('.pbtId0_totdetailDay'+i).text());
                    $('.pbtId110_totdetailDay'+i).text($('.pbtId1_totdetailDay'+i).text());
                    //$('.pbtId22_totdetailDay'+i).text($('.pbtId2_totdetailDay'+i).text());
                    var tncr = 0 ;
                    var tt = 0 ;
                    for(var j=0; j<10; j++){
                        tncr = tncr+parseFloat($('.pbtId22_detailDay'+i+'_'+j).text());
                        tt = tt+parseFloat($('.pbtId22_detailTotal_'+j).text());
                    }
                    $('.pbtId22_totdetailDay'+i).text(tncr);
                    $('.pbtId22_totdetailTotal').text(tt); 
                }
                $('.pbtId01_totdetailTotal').text($('.pbtId0_totdetailTotal').text());
                $('.pbtId110_totdetailTotal').text($('.pbtId1_totdetailTotal').text());
                //$('.pbtId22_totdetailTotal').text($('.pbtId2_totdetailTotal').text());
                
                for(var i=1; i<=7;i++){ 
                    if($('.pbtId5_detailDay'+i+'_0').text().trim() !== '0' && $('.pbtId4_detailDay'+i+'_1').text().trim() !== '0'){
                        $('.pbtId5_detailDay'+i+'_2').text((((parseFloat($('.pbtId5_detailDay'+i+'_0').text()))/(parseFloat($('.pbtId4_detailDay'+i+'_1').text()))*100).toFixed(2))+'%');
                    }else{
                        $('.pbtId5_detailDay'+i+'_2').text('0%');
                    }
                    if($('.pbtId5_detailDay'+i+'_1').text().trim() !== '0' && $('.pbtId4_detailDay'+i+'_2').text().trim() !== '0'){
                        $('.pbtId5_detailDay'+i+'_3').text((((parseFloat($('.pbtId5_detailDay'+i+'_1').text()))/(parseFloat($('.pbtId4_detailDay'+i+'_2').text()))*100).toFixed(2))+'%');
                    }else{
                        $('.pbtId5_detailDay'+i+'_3').text('0%');
                    }
                }
                if($('.pbtId5_detailTotal_0').text().trim() !== '0'){
                    $('.pbtId5_detailTotal_2').text((((parseFloat($('.pbtId5_detailTotal_0').text()))/(parseFloat($('.pbtId4_detailTotal_1').text()))*100).toFixed(2))+'%');    
                }else{
                    $('.pbtId5_detailTotal_2').text('0%');
                }
                if($('.pbtId5_detailTotal_1').text().trim() !== '0'){
                    $('.pbtId5_detailTotal_3').text((((parseFloat($('.pbtId5_detailTotal_1').text()))/(parseFloat($('.pbtId4_detailTotal_2').text()))*100).toFixed(2))+'%');    
                }else{
                    $('.pbtId5_detailTotal_3').text('0%');
                } 
                for(var i=1;i<=7;i++){ 
                    var ttIOS = parseFloat($('.pbtId5_detailDay'+i+'_0').text())+parseFloat($('.pbtId5_detailDay'+i+'_1').text()); 
                    var ttNPS = parseFloat($('.pbtId4_detailDay'+i+'_1').text())+parseFloat($('.pbtId4_detailDay'+i+'_2').text());  
                    if(ttIOS !== 0 && ttNPS !== 0){    
                        $('.pbtId5_totdetailDay'+i).text(((ttIOS/ttNPS*100).toFixed(2))+'%');
                    }else{ 
                        $('.pbtId5_totdetailDay'+i).text('0%');
                    }
                } 
                var impTT = parseFloat($('.pbtId5_detailTotal_0').text())+parseFloat($('.pbtId5_detailTotal_1').text());
                var npsTT = parseFloat($('.pbtId4_detailTotal_1').text())+parseFloat($('.pbtId4_detailTotal_2').text());
                if(impTT !== 0 && npsTT !== 0){
                     $('.pbtId5_totdetailTotal').text((((impTT/npsTT)*100).toFixed(2))+'%');
                }else{                    
                    $('.pbtId5_totdetailTotal').text('0%');
                }  
            }
        </script>
